{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.89.1",
  "pnpmVersion": "7.9.3",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=18.12.0 <19.0.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "gitPolicy": {},
  "repository": {},
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "allowedProjectTags": [],
  "projects": [
    {
      "packageName": "@repo/app",
      "projectFolder": "packages/app"
    },
    {
      "packageName": "@repo/components",
      "projectFolder": "packages/components"
    }
  ]
}